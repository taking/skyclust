# CMP Environment Variables Example
# Copy this file to .env and update the values for your environment

# Application Environment
CMP_ENV=development

# Server Configuration
CMP_PORT=8080
CMP_HOST=0.0.0.0
CMP_READ_TIMEOUT=30
CMP_WRITE_TIMEOUT=30
CMP_IDLE_TIMEOUT=120

# Database Configuration
CMP_DB_HOST=localhost
CMP_DB_PORT=5432
CMP_DB_USER=cmp_user
CMP_DB_PASSWORD=cmp_password
CMP_DB_NAME=cmp
CMP_DB_SSLMODE=disable
CMP_DB_MAX_CONNS=25
CMP_DB_MIN_CONNS=5

# JWT Configuration
CMP_JWT_SECRET=your-super-secret-jwt-key-change-in-production
CMP_JWT_EXPIRATION=24
CMP_JWT_ISSUER=cmp

# Encryption Configuration
CMP_ENCRYPTION_KEY=your-32-byte-encryption-key-here

# NATS Configuration
CMP_NATS_URL=nats://localhost:4222
CMP_NATS_CLUSTER=cmp-cluster

# Plugins Configuration
CMP_PLUGINS_DIR=plugins

# AWS Provider Configuration
CMP_AWS_ACCESS_KEY=your-aws-access-key
CMP_AWS_SECRET_KEY=your-aws-secret-key
CMP_AWS_REGION=us-east-1
CMP_AWS_ROLE_ARN=arn:aws:iam::123456789012:role/CMPRole

# GCP Provider Configuration
CMP_GCP_PROJECT_ID=your-gcp-project-id
CMP_GCP_CREDENTIALS_FILE=/path/to/credentials.json
CMP_GCP_REGION=us-central1

# OpenStack Provider Configuration
CMP_OPENSTACK_AUTH_URL=http://your-openstack-keystone:5000/v3
CMP_OPENSTACK_USERNAME=your-username
CMP_OPENSTACK_PASSWORD=your-password
CMP_OPENSTACK_PROJECT_ID=your-project-id
CMP_OPENSTACK_REGION=RegionOne

# Proxmox Provider Configuration
CMP_PROXMOX_HOST=your-proxmox-host
CMP_PROXMOX_USERNAME=your-username
CMP_PROXMOX_PASSWORD=your-password
CMP_PROXMOX_REALM=pve
