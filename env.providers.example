# SkyClust Cloud Provider Services Configuration
# Copy this file to .env.providers and fill in your credentials

# ========================================
# AWS Provider Configuration
# ========================================
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=us-east-1

# ========================================
# GCP Provider Configuration
# ========================================
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=us-central1
GCP_CREDENTIALS_PATH=/path/to/gcp-credentials.json

# ========================================
# OpenStack Provider Configuration
# ========================================
OS_AUTH_URL=http://your-openstack:5000/v3
OS_USERNAME=your-username
OS_PASSWORD=your-password
OS_PROJECT_ID=your-project-id
OS_REGION_NAME=RegionOne

# ========================================
# Proxmox Provider Configuration
# ========================================
PROXMOX_HOST=https://your-proxmox-host:8006
PROXMOX_USER=your-username
PROXMOX_PASSWORD=your-password
PROXMOX_REALM=pve

